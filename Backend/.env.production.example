########################################
# Backend .env production example (sanitized)
#
# Copy to Backend/.env.production and inject real secrets
# through your secret manager (Azure Container Apps, etc.).
# This file intentionally mirrors the keys expected by
# `config.py` while leaving values blank.
########################################

ENVIRONMENT=production
DATABASE_URL=postgresql+psycopg2://USER:PASSWORD@HOST:5432/DATABASE?sslmode=require
JWT_SECRET=
RESET_SECRET=
SECRET_KEY=
FRONTEND_URL=https://app.example.com
ALLOWED_ORIGINS=https://app.example.com
ALLOWED_ORIGIN_REGEX=
YOCO_SECRET_KEY=
YOCO_WEBHOOK_SECRET=
SENDGRID_API_KEY=
RESET_EMAIL_FROM=alerts@example.com
SENTRY_DSN=
CSP_POLICY=
ENABLE_DEV_DANGEROUS=false
ENABLE_DEV_JOBS=false
ENABLE_DEV_RATE_LIMITS=false
ENABLE_DEV_AUDIT_VIEW=false
ENABLE_DB_CORS_ALLOWLIST=true
RATE_LIMIT_PUBLIC_META_CAPACITY=60
RATE_LIMIT_PUBLIC_META_WINDOW=60
RATE_LIMIT_USER_TENANT_CAPACITY=30
RATE_LIMIT_USER_TENANT_WINDOW=60
RATE_LIMIT_GLOBAL_CAPACITY=120
RATE_LIMIT_GLOBAL_WINDOW=60
ENABLE_RATE_LIMIT_OVERRIDES=false
ENABLE_RATE_LIMIT_PENALTIES=true
DEFAULT_TENANT=default
