  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite",
  "build": "SKIP_CYPRESS_INSTALL=1 NODE_OPTIONS=\"--max-old-space-size=2048\" tsc -b && NODE_OPTIONS=\"--max-old-space-size=2048\" vite build",
